{
  "what": "tests of motion test",
  "strict": true,
  "commands": [
    {
      "type": "launch",
      "which": "webkit",
      "what": "used for tests on which chromium fails on some URLs"
    },
    {
      "type": "url",
      "which": "file://__dirname/testTargets/motion/good.html",
      "what": "page without motion"
    },
    {
      "type": "test",
      "which": "motion",
      "what": "spontaneous change of content",
      "delay": 2400,
      "interval": 2600,
      "count": 5,
      "expect": {
        "meanLocalRatio": 1,
        "maxLocalRatio": 1,
        "globalRatio": 1,
        "meanPixelChange": 0,
        "maxPixelChange": 0,
        "changeFrequency": 0
      }
    },
    {
      "type": "url",
      "which": "file://__dirname/testTargets/motion/bad.html",
      "what": "page with motion"
    },
    {
      "type": "test",
      "which": "motion",
      "what": "spontaneous change of content",
      "delay": 2400,
      "interval": 2600,
      "count": 5,
      "expect": {
        "meanLocalRatio": [">", 1],
        "maxLocalRatio": [">", 1],
        "globalRatio": [">", 1],
        "meanPixelChange": [">", 0],
        "maxPixelChange": [">", 0],
        "changeFrequency": [">", 0]
      }
    }
  ]
}
