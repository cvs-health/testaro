{
  "rule": "dupAtt",
  "timeLimit": 20,
  "acts": [
    {
      "type": "url",
      "which": "file://validation/tests/targets/dupAtt/good.html",
      "what": "page without duplicate attributes"
    },
    {
      "type": "test",
      "which": "testaro",
      "withItems": true,
      "stopOnFail": true,
      "expect": [
        [
          "standardResult.totals.2",
          "=",
          0
        ],
        [
          "standardResult.instances.length",
          "=",
          0
        ]
      ],
      "rules": [
        "y",
        "dupAtt"
      ]
    },
    {
      "type": "url",
      "which": "file://validation/tests/targets/dupAtt/bad.html",
      "what": "page with duplicate attributes"
    },
    {
      "type": "test",
      "which": "testaro",
      "withItems": true,
      "stopOnFail": true,
      "expect": [
        [
          "standardResult.totals.2",
          "=",
          2
        ],
        [
          "standardResult.instances.0.ruleID",
          "=",
          "dupAtt"
        ],
        [
          "standardResult.instances.0.what",
          "i",
          "attributes with the same name"
        ],
        [
          "standardResult.instances.0.tagName",
          "=",
          "P"
        ],
        [
          "standardResult.instances.1.tagName",
          "=",
          "BUTTON"
        ],
        [
          "standardResult.instances.1.id",
          "=",
          "twiceLabeled"
        ],
        [
          "standardResult.instances.1.location.doc",
          "=",
          "source"
        ],
        [
          "standardResult.instances.1.location.type",
          "=",
          "selector"
        ],
        [
          "standardResult.instances.1.location.spec",
          "=",
          "#twiceLabeled"
        ],
        [
          "standardResult.instances.1.excerpt",
          "i",
          "aria-label"
        ]
      ],
      "rules": [
        "y",
        "dupAtt"
      ]
    },
    {
      "type": "test",
      "which": "testaro",
      "withItems": false,
      "stopOnFail": true,
      "expect": [
        [
          "standardResult.totals.2",
          "=",
          2
        ],
        [
          "standardResult.instances.0.ruleID",
          "=",
          "dupAtt"
        ],
        [
          "standardResult.instances.0.what",
          "i",
          "attributes have the same name"
        ],
        [
          "standardResult.instances.0.count",
          "=",
          2
        ]
      ],
      "rules": [
        "y",
        "dupAtt"
      ]
    }
  ]
}
