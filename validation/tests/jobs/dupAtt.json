    },
    {
      "type": "test",
      "which": "dupAtt",
      "what": "elements with duplicate attributes",
      "withItems": true,
      "expect": [
        ["total", "=", 2],
        ["items.0", "=", "p class=\"narrow\" id=\"daParagraph\" class=\"wide\""],
        ["items.1", "i", "large"]
