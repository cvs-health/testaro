{
  "id": "bulk",
  "what": "validation of bulk test",
  "strict": true,
  "timeLimit": 20,
  "acts": [
    {
      "type": "launch",
      "which": "chromium",
      "what": "usual browser"
    },
    {
      "type": "url",
      "which": "__targets__/bulk/good.html",
      "what": "small page"
    },
    {
      "type": "test",
      "which": "bulk",
      "what": "visible element count",
      "expect": [
        ["visibleElements", ">", 1],
        ["visibleElements", "<", 5]
      ]
    },
    {
      "type": "url",
      "which": "__targets__/bulk/bad.html",
      "what": "large page"
    },
    {
      "type": "test",
      "which": "bulk",
      "what": "visible element count",
      "expect": [
        ["visibleElements", ">", 30],
        ["visibleElements", "<", 40]
      ]
    }
  ],
