{
  "id": "title",
  "what": "validation of title test",
  "strict": true,
  "timeLimit": 20,
  "browserID": "webkit",
  "acts": [
    {
      "type": "launch",
      "which": "chromium",
      "url": "file://validation/tests/targets/title/good.html",
      "what": "page with a title"
    },
    {
      "type": "test",
      "which": "testaro",
      "stopOnFail": true,
      "expect": [
        [
          "result.title.data.title",
          "=",
          "Test page"
        ]
      ],
      "withItems": true,
      "rules": [
        "y",
        "title"
      ]
    },
    {
      "type": "url",
      "which": "file://validation/tests/targets/title/bad.html",
      "what": "page without a title"
    },
    {
      "type": "test",
      "which": "testaro",
      "stopOnFail": true,
      "expect": [
        [
          "result.title.data.title",
          "=",
          ""
        ]
      ],
      "withItems": true,
      "rules": [
        "y",
        "title"
      ]
    }
  ],
  "sources": {
  },
  "standard": "no",
  "observe": false,
  "device": {
    "id": "default"
  },
  "timeStamp": "240101T1500",
  "creationTimeStamp": "240101T1200"
}
